{
  "name": "video-sync",
  "version": "0.0.1",
  "description": "Play and switch between multiple video and audio files",
  "main": "main.js",
  "browserslist": "electron 1.6",
  "build": {
    "productName": "ElectronReact",
    "appId": "LeRefugeDesSouvenirs",
    "files": [
      "dist-electron/",
      "node_modules/",
      "main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "scripts": {
    "build": "webpack -p --progress --colors",
    "staging": "webpack -d -p --progress --colors",
    "dev": "webpack-dev-server --hot --inline --colors --port 3000 --open",
    "debug": "webpack-dev-server -d --hot --inline --colors --port 3000 --open",
    "deploy": "git pull origin master && npm run build && cd dist && cp -R ./ /var/www/alpha/ && cd ..",
    "e-prod": "webpack --config webpack.electron-build.config.js && electron --noDevServer .",
    "e-dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.electron-dev.config.js",
    "e-build": "webpack --config webpack.electron-build.config.js",
    "e-package": "webpack --config webpack.electron-build.config.js",
    "e-postpackage": "cross-env DEBUG=* electron-packager ./ --out=./builds --overwrite ",
    "eb-pack": "electron-builder --dir",
    "eb-dist": "electron-builder"
  },
  "devDependencies": {
    "@types/gsap": "1.19.1",
    "@types/node": "8.0.19",
    "@types/react": "16.0.0",
    "@types/react-router": "4.0.15",
    "@types/react-router-dom": "4.0.7",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "css-loader": "0.28.4",
    "electron": "1.6.11",
    "electron-builder": "19.16.0",
    "electron-mocha": "4.0.0",
    "electron-packager": "8.7.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.29.0",
    "istanbul": "0.4.5",
    "minimist": "1.2.0",
    "mocha": "3.4.2",
    "node-sass": "4.5.3",
    "react-youtube": "7.4.0",
    "rollup": "0.45.2",
    "rollup-plugin-istanbul": "1.1.0",
    "sass-loader": "6.0.6",
    "source-map-support": "0.4.15",
    "spectron": "3.7.2",
    "style-loader": "0.18.2",
    "ts-loader": "2.0.0",
    "typescript": "2.3.4",
    "webpack": "3.4.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "axios": "0.16.2",
    "cross-env": "5.0.4",
    "gsap": "1.20.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
    "redux": "3.7.2",
    "redux-actions": "2.2.1"
  },
  "author": "n-kogo",
  "license": "MIT"
}
